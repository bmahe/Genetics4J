<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://maven.apache.org/DECORATION/1.6.0"
	xsi:schemaLocation="http://maven.apache.org/DECORATION/1.6.0
	http://maven.apache.org/xsd/decoration-1.6.0.xsd">
	<bannerLeft>
		<name>${project.name}</name>
		<href>${this.url}</href>
	</bannerLeft>

	<publishDate position="right" format="MMMM dd, yyyy" />
	<version />
	<poweredBy>
		<logo img="#" alt="" />
	</poweredBy>

	<skin>
		<groupId>org.apache.maven.skins</groupId>
		<artifactId>maven-fluido-skin</artifactId>
		<version>1.9</version>
	</skin>

	<custom>
		<fluidoSkin>
		</fluidoSkin>
	</custom>
	
	<body>
		<menu ref="parent"  inherit="top"/>
		<menu ref="modules" inherit="top"/>
		<menu name="Documentation">
			<item name="Quick Start" href="docs/quickstart.html" />
			<item name="Contributing Guilde" href="docs/contributing_guide.html" />
		</menu>
		<menu ref="reports" inherit="top"/>
		<footer />
